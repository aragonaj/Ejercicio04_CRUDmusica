<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Ejercicio03</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Ejercicio03.styles.css" asp-append-version="true" />
    <!-- datatable -->
    @* <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.datatables.net/v/bs5/jq-3.7.0/dt-2.0.7/b-3.0.2/fc-5.0.0/sc-2.4.2/sb-1.7.1/sp-2.3.1/sl-2.0.1/datatables.min.css" rel="stylesheet"> *@
    <link href="https://cdn.datatables.net/v/dt/jq-3.7.0/dt-2.0.7/b-3.0.2/fc-5.0.0/sc-2.4.2/sb-1.7.1/sp-2.3.1/sl-2.0.1/datatables.min.css" rel="stylesheet">
</head>
<body class="body" id="body">
    <header class="header" id="header">
        <div class="header-content" id="header-content">
            <!-- menu-nav-hor - menú navegación horizontal -->
            <a href="Home" class="logo" id="logo">HOME</a>
            <nav class="nav" id="nav">
                <ul class="nav__list menu-navBar">
                    <li class="nav__item menu-li">
                        <a href="#" class="nav__link" id="logIn">LogIn</a>
                    </li>
                    <li class="nav__item menu-li">
                        <a href="#" class="nav__link" id="signUp">SignUp</a>
                    </li>
                </ul>
            </nav>
            <div class="hamburguer" id="hamburguer">
                <div class="bar"></div>
                <div class="bar"></div>
                <div class="bar"></div>
            </div>
            <!-- end menu-nav-hor -->
        </div>
    </header>
    <main class="main">
        <div class="row">
            <section class="seccion contenedor-menuLateral" id="menuLateral">
                <!-- menu-nav-lat - menú navegación lateral -->
                <span class="span" id="titulo-latMenu">TABLAS</span>
                <hr />
                <nav class="nav navLat" id="navLat">
                    <ul class="navLat-menu" id="navLat-menu">
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-albumes" asp-area="" asp-controller="Albumes" asp-action="Index">Álbumes</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-artistas" asp-area="" asp-controller="Artistas" asp-action="Index">Artistas</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-canciones" asp-area="" asp-controller="Canciones" asp-action="Index">Canciones</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-ciudades" asp-area="" asp-controller="Ciudades" asp-action="Index">Ciudades</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-conciertos" asp-area="" asp-controller="Conciertoes" asp-action="Index">Conciertos</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-empleados" asp-area="" asp-controller="Empleadoes" asp-action="Index">Empleados</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-funciones" asp-area="" asp-controller="Funciones" asp-action="Index">Funciones</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-funcionesArtistas" asp-area="" asp-controller="FuncionesArtistas" asp-action="Index">Funciones Artista</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-generos" asp-area="" asp-controller="Generoes" asp-action="Index">Géneros</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-giras" asp-area="" asp-controller="Giras" asp-action="Index">Giras</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-grupos" asp-area="" asp-controller="Grupoes" asp-action="Index">Grupos</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-paises" asp-area="" asp-controller="Paises" asp-action="Index">Países</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-plataformas" asp-area="" asp-controller="Plataformas" asp-action="Index">Plataformas</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-representantes" asp-area="" asp-controller="Representantes" asp-action="Index">Representantes</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-roles" asp-area="" asp-controller="Roles" asp-action="Index">Roles</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-videoclips" asp-area="" asp-controller="VideoClips" asp-action="Index">Videoclips</a>
                            <hr />
                        </li>
                        <li class="navLat-li">
                            <a class="navLat-link" id="link-vcPlataformas" asp-area="" asp-controller="VideoClipsPlataformas" asp-action="Index">Videoclips en plataformas</a>
                            <hr />
                        </li>
                    </ul>
                </nav>
                <!-- end menu-nav-hor -->
            </section>
            <section class="seccion contenido-main" id="contenido-main">
                @RenderBody()
            </section>
        </div>
    </main>
    <footer class="">
        <div class="">
            &copy; 2024 - Ejercicio03 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true" defer></script>
    <!-- datatable -->
    <script src="https://cdn.datatables.net/v/dt/jq-3.7.0/dt-2.0.7/b-3.0.2/fc-5.0.0/sc-2.4.2/sb-1.7.1/sp-2.3.1/sl-2.0.1/datatables.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
